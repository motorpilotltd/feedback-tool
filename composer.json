{
    "name": "motorpilotltd/feedback-tool",
    "type": "project",
    "description": "Feedback Tool.",
    "keywords": [
        "feedback",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1",
        "cviebrock/eloquent-sluggable": "^11.0",
        "diglactic/laravel-breadcrumbs": "^9.0",
        "dyrynda/laravel-cascade-soft-deletes": "^4.4",
        "guzzlehttp/guzzle": "^7.9",
        "itsgoingd/clockwork": "^5.3",
        "laravel/framework": "^11.33",
        "laravel/jetstream": "^5.3",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.5",
        "socialiteproviders/microsoft-azure": "^5.2",
        "socialiteproviders/zoho": "^4.1",
        "spatie/laravel-medialibrary": "^11.10",
        "spatie/laravel-permission": "^6.10",
        "spatie/laravel-settings": "^3.4",
        "staudenmeir/belongs-to-through": "^2.16",
        "wireui/wireui": "^1.19"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.16",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "overtrue/phplint": "^9.3",
        "pestphp/pest": "^2.10",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-livewire": "^2.0",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/site.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": false,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
