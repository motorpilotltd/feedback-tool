{
    "name": "motorpilotltd/feedback-tool",
    "type": "project",
    "description": "Feedback Tool.",
    "keywords": [
        "feedback",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3|^8.4|^8.5",
        "cviebrock/eloquent-sluggable": "^12.0",
        "diglactic/laravel-breadcrumbs": "^10.0",
        "dyrynda/laravel-cascade-soft-deletes": "^4.5",
        "laravel/framework": "^12.52",
        "laravel/jetstream": "^5.4",
        "laravel/sanctum": "^4.3",
        "laravel/socialite": "^5.24",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^4.1",
        "socialiteproviders/microsoft-azure": "^5.2",
        "spatie/laravel-medialibrary": "^11.17",
        "spatie/laravel-permission": "^6.24",
        "spatie/laravel-settings": "^3.6",
        "staudenmeir/belongs-to-through": "^2.17",
        "wireui/wireui": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^4.0",
        "fakerphp/faker": "^1.23",
        "itsgoingd/clockwork": "^5.3",
        "laravel/pint": "^1.16",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "overtrue/phplint": "^9.3",
        "pestphp/pest-plugin-faker": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.0",
        "spatie/laravel-ignition": "^2.9",
        "laravel/pail": "^1.2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/site.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run build"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": false,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
